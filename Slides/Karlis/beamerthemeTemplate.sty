    \node[anchor=north east,text width=\slidewidth-1cm,align=right] at (\slidewidth-0.4cm,3.6cm) {\color{RTUGreen}\tiny\insertsubtitle};
    
    \node[anchor=north east, inner sep=0cm] (unilogo) at (0.965\slidewidth,0.955\slideheight)
    {\includegraphics[width=3.5cm]{themeImages/RTU/rtu_logo_white.png}};
    
   
    \node[anchor=north west, inner sep=0cm] (unilogo) at (0.03\slidewidth,0.97\slideheight)
    {\includegraphics[width=3cm]{themeImages/OtherLogos/CMSlogo_white_nolabel_1024_May2014.png}};
    
    
    
    % \node[above] at(0.5\slidewidth,2.3cm) {\color{black}\tiny by};
    \node at (0.5\slidewidth,2.25cm) {\color{black}\LARGE\textbf{\insertauthor}};
    \node at (0.5\slidewidth,1.5cm) {\color{black}\Large\insertinstitute};

    
    % add the date in the corner
    \node[anchor=south west] at(0,0) {\color{black}\scriptsize\insertdate};

    


    \else
    % NOT the title page
    % title bar
    \fill[color=RTUGreen] (0, \slideheight-1.2cm) rectangle(\slidewidth,\slideheight);
    
    % swap the comment on these to add section titles to slide titles
    \node[anchor=north,text width=\slidewidth-1cm,inner xsep=0.5cm,inner ysep=0.31cm, outer xsep=3.3cm] at (0.5\slidewidth,\slideheight) {\color{white}\LARGE\textbf{\insertframetitle}};
    
   \node[anchor=north east, inner sep=0cm] (unilogo) at (0.99\slidewidth,0.99\slideheight)
    {\includegraphics[width=2cm, trim={1cm, 9cm, 1cm, 1cm}, clip]{themeImages/RTU/rtu_logo_white.png}};

        
    % if we're showing a progress bar, show it
    % (I disable the progress bar and slide numbers for the "Appendix" slides)
    \ifnum \value{showProgressBar}>0\relax%
    % the the progress bar icon in the middle of the screen
    \draw[fill=RTUGreen,draw=none] (0cm,0cm) rectangle(\slidewidth,0.27cm);
	
	\node[anchor=south east] at(\slidewidth,-0.1cm) {\color{white}\tiny\insertframenumber/\inserttotalframenumber};
	%\draw[fill=RTUGreen_lighter,draw=none] (0cm,0cm) rectangle(\msize,0.25cm);
    %\draw[fill=RTUGreen,draw=none] (0cm,0cm) rectangle(\progressbar@tmpdim,0.25cm);
    % NEW add email address on each slide
    
    
    % 2.8 cm added in a few places to fit email address
    \draw[fill=RTUGreen_lighter,draw=none] (2.8cm,0cm) rectangle(\msize,0.25cm);
    \draw[fill=RTUGreen_light,draw=none] (2.8cm,0cm) rectangle(\progressbar@tmpdim+2.8cm-0.8cm,0.25cm);
    \node[anchor=south west] at(0.1cm,-0.1cm) {\color{white}\tiny \emailaddr};
    % bottom information
    % \node[anchor=south west] at(0cm,0.25cm) {\color{TemplateGrey}\tiny\insertsection};
    % if slide numbers are active
    \ifnum \value{showSlideNumbers}>0\relax%
    % if slide totals are active
    \ifnum \value{showSlideTotal}>0\relax%
    % draw both slide number and slide total
    \node[anchor=south east] at(\slidewidth,-0.1cm) {\color{white}\tiny\insertframenumber/\inserttotalframenumber};
    \else
    % slide totals aren't active, don't draw them
    % and make the progress bar dark
     \draw[fill=RTUGreen,draw=none] (2.8cm,0cm) rectangle(\progressbar@tmpdim+2.8cm,0.25cm);
    \node[anchor=south east] at(\slidewidth,-0.1cm) {\color{white}\tiny\insertframenumber};
    \fi
    \fi
    % don't show the progress bar?
    \else
    % section title in the bottom left
    \node[anchor=south west] at(0cm,0cm) {\color{gray}\tiny\insertsection};
    % if we're showing slide numbers
    \ifnum \value{showSlideNumbers}>0\relax%
    % if slide totals are active
    \ifnum \value{showSlideTotal}>0\relax%
    % draw both slide number and slide total
    \node[anchor=south east] at(\slidewidth,0cm) {\color{white}\tiny\insertframenumber/\inserttotalframenumber};
    \else
    % slide totals aren't active, don't draw them
    \node[anchor=south east] at(\slidewidth,0cm) {\color{white}\tiny\insertframenumber};
    \fi
    \fi
    \fi
    \fi
  \end{tikzpicture}
}
